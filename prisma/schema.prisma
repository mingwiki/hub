datasource db {
  provider = "sqlite"
  url      = "file:./core.db"
}
generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
  interface            = "asyncio"
}

model Cache {
  id        Int      @id @default(autoincrement())
  key       String   @unique
  data      Bytes
  updated_at DateTime? @updatedAt
  created_at DateTime @default(now())
}

model Keys {
  id        Int      @id @default(autoincrement())
  key       String   @unique
  data      String
  updated_at DateTime? @updatedAt
  created_at DateTime @default(now())
}

