generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = -1
}

datasource db {
  provider = "sqlite"
  url      = "file:./core.db"
}

model Cache {
  id        Int      @id @default(autoincrement())
  key       String   @unique
  data      Bytes
  access_at DateTime?
  created_at DateTime @default(now())
}

model Keys {
  id        Int      @id @default(autoincrement())
  key       String   @unique
  data      String 
  access_at DateTime?
  created_at DateTime @default(now())
}

model Webhooks {
  id        Int      @id @default(autoincrement())
  key       String   @unique
  data      String 
  sent_at   DateTime?
  created_at DateTime @default(now())
}
